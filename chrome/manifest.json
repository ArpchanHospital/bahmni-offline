{
    "manifest_version": 2,
    "name": "Bahmni",
    "description": "Bahmni is an easy to use, open source hospital system for healthcare providers in low-resource settings. Brought to you by ThoughtWorks.",
    "version": "0.1",
    "icons": {
        "128": "bahmni-128.png"
    },
    "app": {
        "urls": ["https://dev.bahmnidev.org/"],
        "launch": {
            "web_url": "https://dev.bahmnidev.org/bahmni/"
        }
    },
    "permissions": [
        "unlimitedStorage",
        "notifications"
    ],
    "minimum_chrome_version": "28"
}